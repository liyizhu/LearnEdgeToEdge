<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LearnEdgeToEdge"
        tools:targetApi="35">
        <activity
            android:name=".subPage.ImeSystemDefaultActivity"
            android:exported="false" />
        <activity
            android:name=".subPage.ImeOldActivity"
            android:exported="false"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".subPage.ImeActivity"
            android:exported="false"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".subPage.NavigationBarContrastEnforcedActivity"
            android:exported="false" />
        <activity
            android:name=".subPage.ImmersiveModeActivity"
            android:exported="false" />
        <activity
            android:name=".subPage.FitsSystemWindowsActivity"
            android:exported="false" />
        <activity
            android:name=".subPage.GesturesActivity"
            android:exported="false" />
        <activity
            android:name=".subPage.SystemBarInsetsAndDisplayCutoutActivity"
            android:exported="false" />
        <activity
            android:name=".subPage.DisplayCutoutActivity"
            android:exported="false" />
        <activity
            android:name=".subPage.SystemBarInsetsActivity"
            android:exported="false" />
        <activity
            android:name=".subPage.EdgeToEdgeActivity"
            android:exported="false" />
        <activity
            android:name=".subPage.DefaultActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>